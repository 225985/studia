# Ignore all non tex files in doc.
/doc/*
!/doc/*.tex
!/doc/*.pdf
!/doc/*.graffle
!/doc/*.pod
!/doc/*.pptx

.DS_Store
/dist/target
/dist/project/target
/dist/project/project/target
/dist/.ensime
/dist/.ensime_port
/dist/logs/application.log
*.pgm
